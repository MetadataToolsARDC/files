            <rdfs:label>&lt;b&gt;Human Sitting to Walking Transitions: A Motion Capture Dataset&lt;/b&gt;</rdfs:label>
            
            
            <vivo:datePublished rdf:resource="http://openvivo.org/a/date2023-11-08"/>
            
            
